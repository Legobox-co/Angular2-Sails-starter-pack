{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "types": [ "node" ],
  "typeRoots": [ "./node_modules/@types" ],
  "exclude": [
      ".tmp",
      "node_modules",
      "typings"
  ]
}